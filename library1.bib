file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Michelle et al. - 2012 - Proteins associated with the exon junction complex also control the alternative splicing of apoptotic regulator.pdf:pdf},
