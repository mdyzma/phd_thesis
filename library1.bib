keywords = {Animals,Biological,Calcium,Calcium: metabolism,Chickens,Computer Simulation,Heart,Heart: metabolism,Kinetics,Liver,Liver: metabolism,Mitochondria,Models,Rats,Sprague-Dawley,Time Factors},
month = jan,
