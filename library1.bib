file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Atri et al. - 1993 - A single-pool model for intracellular calcium oscillations and waves in the Xenopus laevis oocyte.pdf:pdf},
