issn = {1478-811X},
journal = {Cell communication and signaling : CCS},
month = jan,
number = {1},
pages = {19},
pmid = {21939514},
publisher = {BioMed Central Ltd},
title = {{Calcium signaling around Mitochondria Associated Membranes (MAMs).}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3198985\&tool=pmcentrez\&rendertype=abstract},
