keywords = {Animals,Calcium Signaling,Calcium Signaling: physiology,Electrophysiology,Heart Diseases,Heart Diseases: physiopathology,Humans,Muscle,Neurons,Neurons: physiology,Skeletal,Skeletal: physiology,Smooth,Smooth: physiology},
month = oct,
number = {4},
pages = {1491--545},
