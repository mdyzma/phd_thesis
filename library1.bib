journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Calcium Channels,Calcium Channels: metabolism,Calcium Channels: physiology,Humans,Ion Channel Gating,Ion Channel Gating: physiology,Jurkat Cells,Mitochondria,Mitochondria: physiology,Signal Transduction,Signal Transduction: physiology,T-Lymphocytes,T-Lymphocytes: metabolism,Transcriptional Activation,Transcriptional Activation: physiology},
