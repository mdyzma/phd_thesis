journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Animals,Calcium Channel Blockers,Calcium Channel Blockers: pharmacology,Calcium Channels,Calcium Channels: drug effects,Dose-Response Relationship,Drug,Ganglia,Hippocampus,Hippocampus: drug effects,Neurons,Neurons: drug effects,Nimodipine,Nimodipine: pharmacology,Peptides,Peptides: pharmacology,Pyramidal Cells,Pyramidal Cells: drug effects,Rats,Sympathetic,Sympathetic: drug effects,omega-Conotoxins},
