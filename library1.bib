issn = {0028-1298},
journal = {Naunyn-Schmiedeberg's archives of pharmacology},
keywords = {Animals,Calcium Channel Blockers,Calcium Channel Blockers: pharmacology,Calcium Channels,Calcium Channels: drug effects,Electric Stimulation,Guinea Pigs,Heart Atria,Heart Atria: drug effects,Male,Mibefradil,Mibefradil: pharmacology,Muscle Contraction,Muscle Contraction: drug effects,Rats,Sprague-Dawley,Sympathetic Nervous System,Sympathetic Nervous System: drug effects,Sympathetic Nervous System: physiology,Vas Deferens,Vas Deferens: drug effects,omega-Conotoxin GVIA,omega-Conotoxin GVIA: pharmacology},
month = nov,
