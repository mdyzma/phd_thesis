keywords = {Calcium,Calcium Signaling,Calcium: metabolism,Cell Line,Cell Membrane,Cell Membrane: metabolism,Humans,Ion Transport},
month = feb,
number = {5},
pages = {1392--6},
pmid = {14734814},
title = {{Control of calcium oscillations by membrane fluxes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=337063\&tool=pmcentrez\&rendertype=abstract},
volume = {101},
