journal = {Biophysical journal},
keywords = {Biophysical Phenomena,Biophysics,Cell Membrane,Cell Membrane: metabolism,Diffusion,Kinetics,Models,Neurospora crassa,Neurospora crassa: metabolism,Porins,Porins: chemistry,Protein Conformation,Salts,Salts: chemistry,Salts: pharmacology,Static Electricity,Statistical,Voltage-Dependent Anion Channels},
month = apr,
number = {4},
pages = {1773--83},
