issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Animals,Calcium,Calcium: metabolism,Kinetics,Male,Membrane Potentials,Microscopy, Confocal,Microscopy, Electron, Transmission,Mitochondria,Mitochondria: metabolism,Models, Biological,Myocardium,Myocardium: metabolism,Protein Structure, Tertiary,Rats,Rats, Sprague-Dawley,Ryanodine Receptor Calcium Release Channel,Ryanodine Receptor Calcium Release Channel: metabo,Sarcoplasmic Reticulum,Sarcoplasmic Reticulum: metabolism,Thapsigargin,Thapsigargin: chemistry},
