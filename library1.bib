journal = {Biochimica et biophysica acta},
keywords = {Animals,Cell Death,Cytoskeleton,Cytoskeleton: metabolism,Humans,Inflammation Mediators,Inflammation Mediators: immunology,Necrosis,Necrosis: pathology,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Signal Transduction},
number = {9-10},
pages = {1371--87},
pmid = {16950166},
title = {{Necrosis, a well-orchestrated form of cell demise: signalling cascades, important mediators and concomitant immune response.}},
