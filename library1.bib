issn = {0031-9333},
journal = {Physiological reviews},
keywords = {Animals,Calcium,Calcium Signaling,Calcium Signaling: physiology,Calcium-Transporting ATPases,Calcium-Transporting ATPases: physiology,Calcium: metabolism,Disease,Health,Homeostasis,Humans,Plasma Membrane Calcium-Transporting ATPases,Plasma Membrane Calcium-Transporting ATPases: phys,Protein Conformation,Sarcoplasmic Reticulum Calcium-Transporting ATPase},
month = oct,
