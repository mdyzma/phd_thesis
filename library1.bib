issn = {1471-2164},
journal = {BMC genomics},
keywords = {Algorithms,Mutation,Nucleic Acid Conformation,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,RNA,RNA: chemistry,RNA: genetics},
month = jan,
number = {Suppl 4},
pages = {S6},
pmid = {22759654},
publisher = {BioMed Central Ltd},
title = {{Evaluating our ability to predict the structural disruption of RNA by SNPs.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3303743\&tool=pmcentrez\&rendertype=abstract},
volume = {13 Suppl 4},
