journal = {Mathematical biosciences},
keywords = {Algorithms,Animals,Calcium,Calcium Channels,Calcium Channels: physiology,Calcium Signaling,Calcium Signaling: physiology,Calcium-Transporting ATPases,Calcium-Transporting ATPases: metabolism,Calcium: metabolism,Computer Simulation,Endoplasmic Reticulum,Endoplasmic Reticulum: metabolism,Epithelial Cells,Epithelial Cells: metabolism,Inositol 1,4,5-Trisphosphate,Inositol 1,4,5-Trisphosphate Receptors,Inositol 1,4,5-Trisphosphate Receptors: physiology,Inositol 1,4,5-Trisphosphate: metabolism,Kidney Tubules,Kidney Tubules: metabolism,Kinetics,Models, Biological,Rats},
