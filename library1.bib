journal = {Biochemistry. Biokhimii͡a},
keywords = {Animals,Calcium,Calcium Signaling,Calcium Signaling: physiology,Calcium: metabolism,Calcium: physiology,Cell Membrane,Cell Membrane: metabolism,Endoplasmic Reticulum,Endoplasmic Reticulum: metabolism,Mitochondria,Mitochondria: metabolism},
month = feb,
