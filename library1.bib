journal = {Human mutation},
keywords = {Computational Biology,Computational Biology: methods,Databases, Genetic,Genetic Predisposition to Disease,Genetic Predisposition to Disease: genetics,Humans,Mutation, Missense,Mutation, Missense: genetics},
