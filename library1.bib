keywords = {Animals,Caffeine,Caffeine: pharmacology,Calcium,Calcium: physiology,Cell Surface,Cell Surface: physiology,Cytosol,Cytosol: physiology,Endoplasmic Reticulum,Endoplasmic Reticulum: physiology,Homeostasis,Humans,Intracellular Membranes,Intracellular Membranes: physiology,Periodicity,Phosphatidylinositols,Phosphatidylinositols: physiology,Receptors},
