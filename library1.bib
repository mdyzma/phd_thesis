journal = {Biophysical journal},
keywords = {Algorithms,Animals,Biological,Calcium,Calcium Channels,Calcium Channels: metabolism,Calcium: metabolism,Chemical,Heart,Heart: metabolism,Kinetics,Liver,Liver: metabolism,Mitochondria,Models,Rats,Signal Transduction,Thermodynamics},
