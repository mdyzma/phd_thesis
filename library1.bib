journal = {Cell calcium},
keywords = {Calcium,Calcium Signaling,Calcium: metabolism,Humans,Membrane Potential,Mitochondria,Mitochondria: metabolism,Mitochondrial,Mitochondrial Proteins,Mitochondrial Proteins: metabolism,RNA Interference,Sodium-Calcium Exchanger,Sodium-Calcium Exchanger: antagonists \& inhibitors,Sodium-Calcium Exchanger: genetics,Sodium-Calcium Exchanger: metabolism},
