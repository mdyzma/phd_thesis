journal = {Biophysical chemistry},
keywords = {Animals,Biological,Calcium Signaling,Endoplasmic Reticulum,Endoplasmic Reticulum: metabolism,Endoplasmic Reticulum: physiology,Intracellular Membranes,Intracellular Membranes: physiology,Membrane Potentials,Models,Xenopus},
