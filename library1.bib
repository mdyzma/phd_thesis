file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hoth, Fanger, Lewis - 1997 - Mitochondrial regulation of store-operated calcium signaling in T lymphocytes.pdf:pdf},
