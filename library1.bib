journal = {Cell calcium},
keywords = {Animals,Calcium Signaling,Fertilization,Humans,Ovum,Ovum: metabolism,Ovum: physiology,Phosphoinositide Phospholipase C,Phosphoinositide Phospholipase C: metabolism},
month = jan,
number = {1},
pages = {55--62},
pmid = {23218672},
title = {{PLC$\zeta$ and the initiation of Ca$^{2+}$ oscillations in fertilizing mammalian eggs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23218672},
