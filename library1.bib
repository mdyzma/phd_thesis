journal = {Journal of bioenergetics and biomembranes},
keywords = {Animals,Apoptosis,Apoptosis: physiology,Cell Survival,Cell Survival: physiology,Cytosol,Cytosol: metabolism,Down-Regulation,Down-Regulation: physiology,Energy Metabolism,Energy Metabolism: physiology,Humans,Mitochondria,Mitochondria: metabolism,Mitochondrial Membranes,Mitochondrial Membranes: metabolism,Mitochondrial Proteins,Mitochondrial Proteins: metabolism,Proto-Oncogene Proteins c-bcl-2,Proto-Oncogene Proteins c-bcl-2: metabolism,Signal Transduction,Signal Transduction: physiology,Voltage-Dependent Anion Channel 1,Voltage-Dependent Anion Channel 1: metabolism},
