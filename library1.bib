journal = {Biophysical journal},
keywords = {Animals,Biological Clocks,Biophysical Phenomena,Biophysics,Calcium,Calcium Channels,Calcium Channels: metabolism,Calcium: metabolism,Cytosol,Cytosol: metabolism,Diffusion,Female,Inositol 1,4,5-Trisphosphate,Inositol 1,4,5-Trisphosphate Receptors,Inositol 1,4,5-Trisphosphate: metabolism,Kinetics,Models, Biological,Oocytes,Oocytes: metabolism,Photolysis,Receptors, Cytoplasmic and Nuclear,Receptors, Cytoplasmic and Nuclear: metabolism,Xenopus laevis},
