doi = {10.1042/BC20050032},
file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dupont, Combettes - 2006 - Modelling the effect of specific inositol 1,4,5-trisphosphate receptor isoforms on cellular Ca2 signals.pdf:pdf},
issn = {0248-4900},
journal = {Biology of the cell / under the auspices of the European Cell Biology Organization},
keywords = {Animals,Calcium,Calcium Channels,Calcium Channels: physiology,Calcium Signaling,Calcium: metabolism,Calcium: physiology,Inositol 1,4,5-Trisphosphate Receptors,Intracellular Membranes,Intracellular Membranes: metabolism,Models, Biological,Models, Theoretical,Protein Isoforms,Protein Isoforms: metabolism,Receptors, Cytoplasmic and Nuclear,Receptors, Cytoplasmic and Nuclear: physiology},
