journal = {Biophysical journal},
keywords = {Anions,Electric Conductivity,Hydrostatic Pressure,Intracellular Membranes,Intracellular Membranes: physiology,Ion Channel Gating,Ion Channel Gating: drug effects,Kinetics,Membrane Potentials,Membrane Potentials: drug effects,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: drug effects,Membrane Proteins: physiology,Mitochondria,Mitochondria: physiology,Models, Biological,Neurospora crassa,Neurospora crassa: physiology,Porins,Potassium Chloride,Potassium Chloride: pharmacology,Thermodynamics,Voltage-Dependent Anion Channels},
