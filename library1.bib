issn = {1432-2013},
journal = {Pfl\"{u}gers Archiv : European journal of physiology},
keywords = {Animals,Calcium,Calcium Signaling,Calcium: metabolism,Cytosol,Cytosol: metabolism,Humans,Membrane Potential,Mitochondria,Mitochondria: metabolism,Mitochondrial,Proton-Motive Force,Protons},
month = jul,
number = {1},
pages = {19--26},
pmid = {22526460},
