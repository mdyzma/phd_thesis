title = {{Apoptosis and autophagy: decoding calcium signals that mediate life or death.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2944358\&tool=pmcentrez\&rendertype=abstract},
volume = {2},
year = {2010}
}
@article{Joucla2010,
abstract = {Measuring variations of intracellular free calcium concentration through the changes in fluorescence of a calcium-sensitive dye is a ubiquitous technique in neuroscience. Despite its popularity, confidence intervals (CIs) on the estimated parameters of calcium dynamics models are seldom given. To address this issue, we have developed a two-stage model for ratiometric measurements obtained with a charge-coupled device (CCD) camera. Its first element embeds a parametric calcium dynamics model into a fluorescence intensity model and its second element probabilistically describes the fluorescence measurements by a CCD camera. Using Monte Carlo simulations, we first show that the classical ratiometric transformation gives reliable CIs for time constants only and not baseline calcium concentration nor influx. We then introduce a direct method, which consists of fitting directly and simultaneously the fluorescence transients at both wavelengths, without any data ratioing. This approach uses a probabilistic description of the camera, leading to the construction of meaningful CIs for the calcium parameters. Moreover, using approaches inspired by constrained linear regression, we can take into account the finite precision on calibrated parameters (such as the dye dissociation constant in the cell). These key features are illustrated on simulated data using Monte Carlo simulations. Moreover, we illustrate the strength of the direct method on experimental recordings from insect olfactory interneurons. In particular, we show how to handle a time-dependent buffer concentration, thereby considerably improving our goodness of fit. The direct method was implemented in the open-source software R and is freely distributed in the CalciOMatic package.},
author = {Joucla, S\'{e}bastien and Pippow, Andreas and Kloppenburg, Peter and Pouzat, Christophe},
doi = {10.1152/jn.00414.2009},
file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Joucla et al. - 2010 - Quantitative estimation of calcium dynamics from ratiometric measurements a direct, nonratioing method.pdf:pdf},
issn = {1522-1598},
journal = {Journal of neurophysiology},
keywords = {Animals,Calcium,Calcium Signaling,Calcium Signaling: physiology,Calcium: analysis,Calcium: metabolism,Cells, Cultured,Computer Simulation,Humans,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Models, Neurological,Neurons,Neurons: physiology},
month = feb,
number = {2},
pages = {1130--44},
pmid = {19955286},
title = {{Quantitative estimation of calcium dynamics from ratiometric measurements: a direct, nonratioing method.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19955286},
volume = {103},
year = {2010}
}
@article{Mazel2009,
abstract = {Release of inflammatory mediators by mast cells in type 1 immediate-hypersensitivity allergic reactions relies on antigen-dependent increases in cytosolic calcium. Here, we used a series of electron microscopy images to build a 3D reconstruction representing a slice through a rat tumor mast cell, which then served as a basis for stochastic modeling of inositol-trisphosphate-mediated calcium responses. The stochastic approach was verified by reaction-diffusion modeling within the same geometry. Local proximity of the endoplasmic reticulum to either the plasma membrane or mitochondria is predicted to differentially impact local inositol trisphosphate receptor transport. The explicit consideration of organelle spatial relationships represents an important step toward building a comprehensive, realistic model of cellular calcium dynamics.},
