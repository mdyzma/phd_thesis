keywords = {Calcium,Calcium Channels,Calcium-Binding Proteins,Calcium-Binding Proteins: metabolism,Calcium: metabolism,Calcium: physiology,Hydrogen-Ion Concentration,Intracellular Membranes,Intracellular Membranes: metabolism,Intracellular Membranes: physiology,Mathematical Computing,Membrane Potentials,Membrane Potentials: physiology,Mitochondria,Mitochondria: metabolism,Mitochondria: physiology,Models, Biological,Oxygen Consumption,Oxygen Consumption: physiology,Ruthenium Red,Ruthenium Red: pharmacokinetics,Signal Transduction,Signal Transduction: physiology},
