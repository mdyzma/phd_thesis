journal = {Cell calcium},
keywords = {Animals,Calcium,Calcium Channels,Calcium Channels: metabolism,Calcium: metabolism,Mitochondria,Mitochondria: metabolism,Mitochondrial Membrane Transport Proteins,Mitochondrial Membrane Transport Proteins: metabol,Ruthenium Red},
month = jul,
