keywords = {Animals,Benzofurans,Calcium,Calcium: metabolism,Cell Line,Cytotoxicity, Immunologic,Fura-2,Lymphocyte Activation,Mice,Mice, Inbred CBA,T-Lymphocytes, Cytotoxic,T-Lymphocytes, Cytotoxic: immunology,T-Lymphocytes, Cytotoxic: metabolism},
