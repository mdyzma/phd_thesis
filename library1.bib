journal = {Journal of theoretical biology},
keywords = {Animals,Biological Clocks,Biological Clocks: physiology,Biological Transport,Biological Transport: physiology,Calcium,Calcium Signaling,Calcium Signaling: physiology,Calcium: metabolism,Homeostasis,Homeostasis: physiology,Hydrogen-Ion Concentration,Membrane Potentials,Membrane Potentials: physiology,Mitochondria,Mitochondria: metabolism,Mitochondrial Membranes,Mitochondrial Membranes: metabolism,Models, Biological,Permeability},
