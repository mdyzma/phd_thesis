issn = {0146-0404},
journal = {Investigative ophthalmology \& visual science},
keywords = {Animals,Caffeine,Caffeine: pharmacology,Calcimycin,Calcimycin: pharmacology,Calcium,Calcium: metabolism,Chlorophenols,Chlorophenols: pharmacology,Citrate (si)-Synthase,Citrate (si)-Synthase: metabolism,Electron Transport Complex IV,Electron Transport Complex IV: metabolism,Gene Expression,Male,Mitochondria, Muscle,Mitochondria, Muscle: drug effects,Mitochondria, Muscle: metabolism,Muscle Contraction,Muscle Contraction: physiology,Oculomotor Muscles,Oculomotor Muscles: metabolism,RNA, Messenger,RNA, Messenger: metabolism,Rats,Rats, Sprague-Dawley,Reverse Transcriptase Polymerase Chain Reaction,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism},
