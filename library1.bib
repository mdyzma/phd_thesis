journal = {The Journal of physiology},
keywords = {Calcium,Calcium: metabolism,Egtazic Acid,Egtazic Acid: metabolism,Mathematics,Mitochondria, Heart,Mitochondria, Heart: metabolism,Models, Biological,Protons,Reproducibility of Results,Sodium,Sodium-Calcium Exchanger,Sodium-Calcium Exchanger: metabolism,Sodium: metabolism},
month = jul,
