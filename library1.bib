month = jan,
number = {8},
pages = {17--19},
pmid = {20700497},
title = {{Calcium signals driven by single channel noise.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2917103\&tool=pmcentrez\&rendertype=abstract},
