journal = {Archives of biochemistry and biophysics},
keywords = {Alternative Splicing,Animals,Calcium,Calcium: metabolism,Cell Membrane,Cell Membrane: enzymology,Humans,Models,Molecular,Plasma Membrane Calcium-Transporting ATPases,Plasma Membrane Calcium-Transporting ATPases: chem,Plasma Membrane Calcium-Transporting ATPases: phys,Protein Conformation,Protein Isoforms,Protein Isoforms: chemistry,Protein Isoforms: physiology},
month = aug,
