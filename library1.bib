journal = {Biochimica et biophysica acta},
keywords = {Animals,Calcium,Calcium: metabolism,Humans,Ion Transport,Mitochondria,Mitochondria: metabolism,Mitochondrial Membrane Transport Proteins,Reactive Oxygen Species,Ryanodine Receptor Calcium Release Channel,Ryanodine Receptor Calcium Release Channel: physio,Sodium,Sodium: metabolism},
