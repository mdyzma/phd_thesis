keywords = {Biophysical Phenomena,Biophysics,Buffers,Calcium,Calcium Channels,Calcium Channels: metabolism,Calcium: chemistry,Calcium: metabolism,Coloring Agents,Ion Transport,Mathematics,Models, Biological},
month = jun,
number = {6},
pages = {2527--39},
pmid = {8744292},
title = {{Validity of the rapid buffering approximation near a point source of calcium ions.}},
