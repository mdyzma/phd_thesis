file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tang, Stephenson - 1996 - Calcium dynamics and homeostasis in a mathematical model of the principal cell of the cortical collecting tubu.pdf:pdf},
