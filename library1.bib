issn = {0014-5793},
journal = {FEBS letters},
keywords = {Animals,Biological Transport,Calcium,Calcium: metabolism,Calcium: physiology,Endoplasmic Reticulum,Endoplasmic Reticulum: metabolism,Humans,Mitochondria,Mitochondria: metabolism,Oxygen,Oxygen: metabolism,Permeability,Phosphorylation,Sarcoplasmic Reticulum,Sarcoplasmic Reticulum: metabolism,Time Factors},
