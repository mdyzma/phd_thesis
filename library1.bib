doi = {10.1042/BJ20070619},
file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lytton - 2007 - NaCa2 exchangers three mammalian gene families control Ca2 transport.pdf:pdf},
issn = {1470-8728},
journal = {The Biochemical journal},
keywords = {Animals,Brain,Brain: metabolism,Calcium,Calcium Signaling,Calcium: metabolism,Humans,Kidney,Kidney: metabolism,Multigene Family,Multigene Family: physiology,Muscle,Myocardium,Myocardium: metabolism,Potassium,Potassium: metabolism,Skeletal,Skeletal: metabolism,Sodium,Sodium-Calcium Exchanger,Sodium-Calcium Exchanger: physiology,Sodium: metabolism},
