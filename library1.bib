keywords = {Animals,Apoptosis,Cell Cycle,Cell Cycle: physiology,Dimerization,Humans,Oncogenes,Proto-Oncogene Proteins c-bcl-2,Proto-Oncogene Proteins c-bcl-2: physiology},
