file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Smith, Wagner, Keizer - 1996 - Validity of the rapid buffering approximation near a point source of calcium ions.pdf:pdf},
