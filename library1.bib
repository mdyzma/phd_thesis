journal = {Cell calcium},
keywords = {4,5-Trisphosphate,5-Trisphosphate: pharmacology,Biological,Calcium,Calcium Channels,Calcium Channels: chemistry,Calcium Channels: drug effects,Calcium: metabolism,Cerebellum,Cerebellum: metabolism,Computer Simulation,Cytosol,Cytosol: metabolism,Diffusion,Fluorescent Dyes,Fluorescent Dyes: metabolism,Inositol 1,Ion Channel Gating,Ion Transport,Kinetics,Lipid Bilayers,Lipid Bilayers: metabolism,Models,Organic Chemicals,Stochastic Processes},
