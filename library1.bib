pmid = {21478166},
title = {{The RNAmute web server for the mutational analysis of RNA secondary structures.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3125725\&tool=pmcentrez\&rendertype=abstract},
