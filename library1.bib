journal = {Brain research},
keywords = {Animals,Buffers,Calcium,Calcium: metabolism,Carbonyl Cyanide m-Chlorophenyl Hydrazone,Carbonyl Cyanide m-Chlorophenyl Hydrazone: pharmac,Clonazepam,Clonazepam: pharmacology,Cyclosporine,Cyclosporine: pharmacology,Male,Mitochondria,Mitochondria: drug effects,Mitochondria: metabolism,Mitochondria: physiology,Neurons,Neurons: metabolism,Permeability,Prosencephalon,Prosencephalon: cytology,Prosencephalon: metabolism,Rats,Rats, Inbred F344,Sodium-Calcium Exchanger,Sodium-Calcium Exchanger: antagonists \& inhibitors,Sodium-Calcium Exchanger: metabolism},
