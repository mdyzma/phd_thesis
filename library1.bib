file = {:C$\backslash$:/Users/Michal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vianello et al. - 2012 - The mitochondrial permeability transition pore (PTP) - an example of multiple molecular exaptation.pdf:pdf},
issn = {0006-3002},
journal = {Biochimica et biophysica acta},
keywords = {Animals,Calcium,Calcium: metabolism,Evolution, Molecular,Humans,Mitochondrial Membrane Transport Proteins,Mitochondrial Membrane Transport Proteins: genetic,Mitochondrial Membrane Transport Proteins: physiol,Phylogeny,Potassium Channels,Potassium Channels: physiology,Reactive Oxygen Species,Reactive Oxygen Species: metabolism},
month = nov,
